       to_char(ALT_SHARE_AMOUNT, pms_p.get_parameter('AMOUNT_FORMAT')) ALT_SHARE_AMOUNT,
       MEMBERSHIP_FOLIO_MESSAGE,
	   addl_addressee_name_id,
       addl_addressee_name,
       addl_addressee_full_address,
       addl_addressee_address1,
       addl_addressee_address2,
       addl_addressee_city,
       addl_addressee_state,
       addl_addressee_country,
       addl_addressee_country_desc,
       addl_addressee_state_desc,
       addl_addressee_zip_code,
       cancellation_date,
       rep_gen.iso_format_from_char(cancellation_Date) cancellation_date_iso,
       cancellation_reason_code,
       cancellation_reason_desc,
       awrd_redeem_upgrd_points,
       is_upsold_yn,
       upsold_by_user_id,
       upsell_rule_id,
       upsell_rule_code,
       org_room_category,
       org_room_category_label,
       org_stay_cost,
       first_night_upsell,
       total_upsell_charge,
       total_stay_cost_after_upsell,
       resort_check_in_time,
       rep_gen.iso_format_from_char(arrival_date, resort_check_in_time) resort_check_in_time_iso,
       resort_check_out_time,
       rep_gen.iso_format_from_char(departure_date, resort_check_out_time) resort_check_out_time_iso,
       opera_ecertificate,
       promotion_coupons,
       loyalty_mem_no,
       loyalty_mem_level_code,
       loyalty_mem_level_desc,
       loyalty_mem_balance,
       loyalty_nights_to_renew,
       loyalty_nights_next_level,
	   confirmation_ref.multiple_rate_info multiple_rate_info,
	   CASE WHEN RESERVATION_TYPE_DESCRIPTION IN ('4 PM HOLD','DEPOSIT REQUESTED','TENTATIVE GRP HOLD') THEN
	             'not guaranteed'
		    ELSE 'guaranteed'
	   END GUARANTEE_STATUS,
	   (select initcap(short_description) from resort_room_category where resort=pms_p.resort and label=CONFIRMATION_GENERAL_VIEW.ROOM_TYPE) CF_ROOMTYPE_DESC,
	   ors_resort,
	   guest_donotemail,
	   guest_donotmail,
	   guest_greeting_long,
	   membership_level,
	   accompanying_names,
	   accompanying_yn,
	   channel,
	   children_age,
	   transaction_code,
	   trx_tax_incl_yn,
	   contact_address_building,
	   contact_address_city, 
	   contact_address_country_code,
	   contact_address_state_code ,
	   contact_address_street,
	   contact_address_zip,
	   booker_crm_id, 
	   contact_name_id,
	   contact_title,
	   contact_firstname,
	   contact_middlename,
	   contact_lastname,
	   contact_gender,
	   contact_email,
	   contact_greeting ,
	   contact_greeting_long,	   
	   travel_agent_commission,
	   routing_codes,
       package_addl_codes,
       packages_inclusive_codes,
	   rtc_room_class
  FROM CONFIRMATION_GENERAL_VIEW
 WHERE RESV_NAME_ID = :P_RESV_NAME_ID
   AND RESORT_NAME = :P_INIT_RESORT
    ]]>
    </sqlStatement>

    <sqlStatement name="Q2">
    <![CDATA[
    SELECT r.resv_name_id act_resv_name_id,
       TO_CHAR(start_date, pms_p.date_format) activity_date,
       o.activity_type activity_type,
       o.location_code activity_location_code,
       o.location_desc activity_location_desc,
       o.status_desc activity_status_desc,
       o.activity_desc,
       o.pax activity_pax,
       o.start_time activity_start_time,
       o.duration activity_duration,
       o.end_time activity_end_time,
       o.status_code activity_status_code,
       o.notes activity_notes,
       o.guest_type activity_guest_type,
       o.activity_amount,
       o.deposit_amount activity_deposit_amount,
       (initcap(r.sguest_name)||' '||initcap(r.sguest_firstname)) activity_guestname
   FROM osa_reservation_bookings o, reservation_name r
   WHERE (r.resv_name_id = :p_resv_name_id or ( r.parent_resv_name_id = :p_resv_name_id and r.name_usage_type = 'AG'))
   AND o.resv_name_id = r.resv_name_id
   AND o.resort = :p_init_resort
   AND :p_activity_license_yn = 'Y'
   ORDER BY o.resv_name_id
    ]]>
    </sqlStatement>

    <sqlStatement name="Q3">
    <![CDATA[
    SELECT ri.resv_name_id item_resv_name_id,
       ri.quantity item_quantity,
       TO_CHAR(ri.begin_date,pms_p.date_format) item_begin_date,
       rep_gen.iso_format(ri.begin_date) item_begin_date_iso,
       TO_CHAR(ri.end_date,pms_p.date_format) item_end_date,
       rep_gen.iso_format(ri.end_date) item_end_date_iso,
       DECODE(:p_guest_language_yn,'Y',NVL(sc_translation.item_trans('NAME',:language_code,gi.item_id,gi.resort),gi.name),gi.name) item_name,
       DECODE(:p_guest_language_yn,'Y',NVL(sc_translation.item_trans('DESCRIPTION',:language_code,gi.item_id,gi.resort),gi.description),gi.description) item_description,
       gi.article_number item_code
    FROM reservation_items ri,
         gem$item gi
    WHERE ri.resort = :p_init_resort
      AND ri.resv_name_id=:p_resv_name_id
      AND gi.resort = ri.resort
      AND gi.item_id = ri.item_id
      AND gi.showbeo_yn = 'Y'
      AND :p_item_inventory_yn = 'Y'
    ORDER BY gi.article_number,ri.begin_date,ri.end_date
    ]]>
    </sqlStatement>

    <sqlStatement name="Q4">
    <![CDATA[
     SELECT
      resv_name_id in_conf_resv_name_id,
      upsell_offer_id,
      to_rate_code,
      from_room_category,
      to_room_category,
      to_room_category_label,
      from_room_class,
      to_room_class,
      first_night_rate_amt,
      total_stay_amt,
      first_night_upsell,
      tot_upsell_money_amt,
      total_ranking_value,
      rate_source_type,
      rate_source,
      membership_type,
      membership_level,
      channel,
      guarantee_type,
      market_code,
      upsell_rule_id,
      upsell_code,
      in_conf_resort,
      first_night_upsell_charge,
      total_upsell_charge,
      room_category_description,
      DECODE(:p_show_description_yn,'Y',resv_pref.get_feat_description(room_features),room_features) room_features,
      v_first_night_amt_fmt,
      v_total_stay_amt_fmt,
      v_first_ngt_upsell_fmt,
      v_tot_upsell_money_fmt,
      v_first_night_chrg_fmt,
      v_tot_upsell_chrg_fmt,
      currency_code,
      room_category_description_long
      FROM confirmation_upsells_view cuv
     WHERE :P_RESERVATION_UPSELL = 'Y'
       AND cuv.resv_name_id = :p_resv_name_id
       AND cuv.in_conf_resort = :p_init_resort
    ]]>
    </sqlStatement>
	
	<sqlStatement name="Q_DEPOSIT_RULE">
    <![CDATA[
        select resv_deposit_schedule_id
		       ,rule_code as  deposit_rule_code 
		       ,description as deposit_rule_desc
         from reservation_deposit_schedule
         where resort=:p_init_resort
		  and  resv_name_id=:p_resv_name_id
    ]]>
    </sqlStatement>
	
	<sqlStatement name="Q_CANCEL_RULE">
    <![CDATA[
       select rcp.resv_cancel_policy_id
		      ,rcp.rule_code  as cancel_rule_code
			  ,rcp.description as cancel_rule_desc
        from reservation_cancel_policy rcp,
		     reservation_name rn
       where rcp.resv_name_id = rn.resv_name_id 
	     and rn.resv_name_id=:p_resv_name_id
		 and rn.resort= rcp.resort
		 and rn.resort= :p_init_resort
    ]]>
    </sqlStatement>
    
    <sqlStatement name="Q_LINKED_RESERVATIONS">
    <![CDATA[
        SELECT
          char1 resort,
          char2 title,    
          char3 guest_name,    
          bigchar1 room_category,    
          char8 confirmation_no,    
          to_char(number1, pms_p.get_parameter('AMOUNT_FORMAT')) nightly_rate,  
          number2 adults,  
          number3 children,  
          date1 arrival, 
          rep_gen.iso_format(date1) arrival_date_iso,   
          date2 departure,
          rep_gen.iso_format(date2) departure_date_iso,  
          confirmation_odt.get_incl_packages(char1,
                                             number4,char7)inclusive_packages,
          char9 link_type,
          char10 resv_status                
        FROM REPORT_TABLE
        WHERE report_id = confirmation_odt.get_report_id
    ]]>
    </sqlStatement>
	
    <sqlStatement name="Q_PRIVACY_OPTIONS">
    <![CDATA[
       SELECT 
	       n.mail_list m_lst,
           n.market_research_yn mr_yn,
           n.third_party_yn tp_yn,
           n.autoenroll_member_yn am_yn,
           n.email_yn e_yn,
           n.phone_yn p_yn,
           n.sms_yn sms_yn,
           n.guest_priv_yn gp_yn,
           n.d_opt_in_mail_list_flg dm_lst,
           n.d_opt_in_market_research_flg dmr_yn,
           n.d_opt_in_third_party_flg dtp_yn,
           n.d_opt_in_autoenroll_member_flg dam_yn,
           n.d_opt_in_email_flg de_yn,
           n.d_opt_in_phone_flg dp_yn,
           n.d_opt_in_sms_flg dsms_yn,
           n.d_opt_in_guest_priv_flg dgp_yn,
           NAME_REF.get_phone_no(n.name_id,'EMAIL' ) email
       FROM name n,
		    reservation_name rn
	   WHERE rn.resv_name_id = :p_resv_name_id
		 AND rn.resort 		 = :p_init_resort
		 AND n.name_id 	     = rn.name_id
    ]]>
    </sqlStatement>	
