<?xml version="1.0" encoding="UTF-8" ?>
<dataTemplate name="DATA" description="ConfirmationLetter"  defaultPackage="confirmation_odt" Version="1.0">
  <parameters>
   <parameter name="FRM_SID" dataType="character" />
  </parameters>

  <dataQuery>

    <sqlStatement name="Q_REPORT">
    <![CDATA[
     select pms_p.resort,pms_p.date_format date_format,
            pms_p.time_format time_format,
            pms_p.get_parameter('AMOUNT_FORMAT') amount_format,
            pms_p.business_date business_date,
            rep_gen.iso_format(pms_p.business_date) business_date_iso,
            rep_gen.get_logo_name logo_image,
            pms_p.currency p_currency,
            (SELECT name FROM resort WHERE resort  = pms_p.resort) resort_name,
            to_char(tz.get_property_date,pms_p.time_format) p_time,
            to_char(tz.get_property_date,pms_p.date_format) p_date,
            rep_gen.iso_format(tz.get_property_date) p_date_iso,
			(SELECT count(r.resv_name_id) 
			   FROM osa_reservation_bookings o, reservation_name r
			  WHERE (r.resv_name_id = :p_resv_name_id or ( r.parent_resv_name_id = :p_resv_name_id and r.name_usage_type = 'AG'))
			    AND o.resv_name_id = r.resv_name_id
			    AND o.resort = :p_init_resort
			    AND :p_activity_license_yn = 'Y') CS_ACTIVITY_COUNT,
			( SELECT count(ri.resv_name_id)
				FROM reservation_items ri,
					 gem$item gi
			   WHERE ri.resort = :p_init_resort
				 AND ri.resv_name_id=:p_resv_name_id
				 AND gi.resort = ri.resort
				 AND gi.item_id = ri.item_id
				 AND gi.showbeo_yn = 'Y'
				 AND :p_item_inventory_yn = 'Y') CS_ITEM_COUNT,
			(SELECT count(upsell_offer_id)
               FROM confirmation_upsells_view cuv
              WHERE :P_RESERVATION_UPSELL = 'Y'
                AND cuv.resv_name_id = :p_resv_name_id
                AND cuv.in_conf_resort = :p_init_resort) CS_UPSELL,
			f_message('SAMPLE_GUEST_CONFIRMATION_TEXT1_'||pms_p.resort)Confirmation_bottom_text1,
			f_message('SAMPLE_GUEST_CONFIRMATION_TEXT2_'||pms_p.resort)Confirmation_bottom_text2,
			f_label('B_MAIL_LIST','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_mail_list,
			f_label('B_MARKET_RESEARCH_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_market_research_yn,
			f_label('B_MARKET_RESEARCH_H_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_market_research_h_yn,
			f_label('B_THIRD_PARTY_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_third_party_yn,
			f_label('B_AUTOENROLL_MEMBER_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_autoenroll_member_yn,
			f_label('B_EMAIL_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_email_yn,
			f_label('B_PHONE_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_phone_yn,
			f_label('B_SMS_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_sms_yn,
			f_label('B_GUEST_PRIV_YN','REP_LBL',NULL,pms_p.user_language,'privacy_options') lbl_guest_priv_yn,
            NVL(pms_p.has_permission_yn('GUEST_PRIVACY_FIELDS'),'N') guest_priv_permis,
			NVl(pms_p.get_parameter('PRIVACY'),'N') priv_permis			
     from dual
    ]]>
    </sqlStatement>

    <sqlStatement name="Q_PARAMETERS">
    <![CDATA[
      select p.name param_name,p.value param_value
      from TABLE(rep_gen.params) p
      where rep_gen.show_parameter(p.name) = 'Y'
      order by 1
    ]]>
    </sqlStatement>

